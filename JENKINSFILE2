pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      steps {
        script {
          sh 'sudo systemctl start docker' // Start Docker daemon with sudo
          sh 'docker build -t jenkins-test .'
          echo 'Image created successfully'
        }
      }
    }
    stage('PrintOutput') {
      steps {
        sh 'echo "PrintOutput runned successfully"'
      }
    }
  }
}
