pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      steps {
        sh 'docker build -t jenkins-test1/jenkins-test1 .'
      }
    }
  }
  post {
    always {
      script {
        // Add Jenkins user to the Docker group
        sh 'sudo usermod -aG docker jenkins'
      }
    }
  }
}
