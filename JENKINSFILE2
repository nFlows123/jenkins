pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      steps {
        script {
          // Start Docker daemon if not already running (assuming Jenkins agent has permission)
          sh 'docker info || service docker start'

          // Build Docker image
          sh 'docker build -t jenkins-test .'
          
          // Print a message indicating success
          echo 'Image created successfully'
        }
      }
    }
    stage('PrintOutput') {
      steps {
        sh 'echo "PrintOutput run successfully"'
      }
    }
  }
}
